# Environment Configuration Example
# Copy this file to .env.local for local development
# Copy to .env.production for production deployment

# Google Analytics Tracking ID
REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX

# API Configuration
REACT_APP_API_URL=https://api.luxurywagyu.com
REACT_APP_API_KEY=your_api_key_here

# Contact Information
REACT_APP_CONTACT_EMAIL=info@luxurywagyu.com
REACT_APP_CONTACT_PHONE=+1-555-0123

# Social Media Links
REACT_APP_FACEBOOK_URL=https://www.facebook.com/luxurywagyu
REACT_APP_INSTAGRAM_URL=https://www.instagram.com/luxurywagyu
REACT_APP_TWITTER_URL=https://www.twitter.com/luxurywagyu

# Business Information
REACT_APP_BUSINESS_NAME=Luxury Wagyu
REACT_APP_BUSINESS_ADDRESS=123 Premium Street, Luxury City, LC 12345

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT_SUPPORT=false
REACT_APP_ENABLE_WHOLESALE_PORTAL=true

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.luxurywagyu.com

# Environment
NODE_ENV=development

